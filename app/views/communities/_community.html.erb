<div id="<%= dom_id community %>">
  <p>
    <strong>Identifier:</strong>
    <%= community.identifier %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= community.name %>
  </p>
  
  <%= image_tag community.avatar rescue "No avatar" %>
  <%= image_tag community.banner rescue "No banner" %>

</div>
